<div class="w3-bar w3-black w3-border w3-card-4" style="border-width: 0 !important;">
    <a href="/" class="w3-bar-item w3-button w3-red"><%=config.author%></a>
    <a href="/archive" class="w3-bar-item w3-button">Archive</a>
    <a href="/page/link" class="w3-bar-item w3-button">Link</a>
    <a href="/page/about" class="w3-bar-item w3-button">About</a>
    <% if(config.nav_links !== undefined && config.nav_links !== ''){
      try {
        let dropdowns = JSON.parse(config.nav_links);
        dropdowns.forEach((dropdown)=>{ %>
        <div class="w3-dropdown-hover w3-hide-small">
            <div class="w3-button"><%= dropdown.key %></div>
            <div class="w3-dropdown-content w3-bar-block w3-card-4">
            <% if (dropdown.value) dropdown.value.forEach((link)=>{ %>
                <a href="<%- link.link %>" class="w3-bar-item w3-button w3-mobile" href="<%- link.link %>" rel="noreferrer"><%- link.text %></a>
            <% }) %>
            </div>
        </div>
    <%})
    } catch (e) {console.error(e.message)}} %>
</div>
